                    <TextBox  x:Name="IB5" Text="{Binding IB5, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True,NotifyOnValidationError=True}" Margin="2,1,0,1"  FontSize="4" Background="Transparent"  />
                </Grid>
                <Grid Grid.Row="8"  Grid.Column="8" Background="{StaticResource brushWatermarkBackground}" >
                    <Border BorderBrush='WhiteSmoke'  BorderThickness='2,1,0,1'/>
                    <TextBlock Margin="5,2" Text="IB6" FontSize="4" Visibility="{Binding ElementName=IB6, Path=Text.IsEmpty, Converter={StaticResource BooleanToVisibilityConverter}}" Foreground="{StaticResource brushWatermarkForeground}"/>
                    <TextBox  x:Name="IB6" Text="{Binding IB6, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True,NotifyOnValidationError=True}" Margin="2,1,0,1"  FontSize="4" Background="Transparent"  />
                </Grid>
                <Grid Grid.Row="8"  Grid.Column="9" Background="{StaticResource brushWatermarkBackground}" >
                    <Border BorderBrush='WhiteSmoke'  BorderThickness='2,1,0,1'/>
                    <TextBlock Margin="5,2" Text="IB7" FontSize="4" Visibility="{Binding ElementName=IB7, Path=Text.IsEmpty, Converter={StaticResource BooleanToVisibilityConverter}}" Foreground="{StaticResource brushWatermarkForeground}"/>
                    <TextBox  x:Name="IB7" Text="{Binding IB7, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True,NotifyOnValidationError=True}"  Margin="2,1,0,1"  FontSize="4" Background="Transparent"  />
                </Grid>
                <Grid Grid.Row="8"  Grid.Column="10" Background="{StaticResource brushWatermarkBackground}" >
                    <Border BorderBrush='WhiteSmoke'  BorderThickness='2,1,0,1'/>
                    <TextBlock Margin="5,2" Text="IB8" FontSize="4" Visibility="{Binding ElementName=IB8, Path=Text.IsEmpty, Converter={StaticResource BooleanToVisibilityConverter}}" Foreground="{StaticResource brushWatermarkForeground}"/>
                    <TextBox  x:Name="IB8" Text="{Binding IB8, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True,NotifyOnValidationError=True}"  Margin="2,1,0,1"  FontSize="4" Background="Transparent"  />
                </Grid>
                <Grid Grid.Row="8"  Grid.Column="11" Background="{StaticResource brushWatermarkBackground}" >
                    <Border BorderBrush='WhiteSmoke'  BorderThickness='2,1,0,1'/>
                    <TextBlock Margin="5,2" Text="IB9" FontSize="4" Visibility="{Binding ElementName=IB9, Path=Text.IsEmpty, Converter={StaticResource BooleanToVisibilityConverter}}" Foreground="{StaticResource brushWatermarkForeground}"/>
                    <TextBox  x:Name="IB9" Text="{Binding IB9, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True,NotifyOnValidationError=True}"  Margin="2,1,0,1"  FontSize="4" Background="Transparent"  />
                </Grid>
                <Grid Grid.Row="8"  Grid.Column="12" Background="{StaticResource brushWatermarkBackground}" >
                    <Border BorderBrush='WhiteSmoke'  BorderThickness='2,1,0,1'/>
                    <TextBlock Margin="5,2" Text="EoS" FontSize="4" Visibility="{Binding ElementName=IB10, Path=Text.IsEmpty, Converter={StaticResource BooleanToVisibilityConverter}}" Foreground="{StaticResource brushWatermarkForeground}"/>
                    <TextBox  x:Name="IB10" Text="{Binding IB10}"  Padding="0,0,0,0" Margin="2,1,0,1"  FontSize="4" IsEnabled="False" Background="Transparent"  />
                </Grid>
                <Grid Row="9"  Column="1">
                    <Border BorderBrush='White'  BorderThickness='1,1,0,0'>
                        <Border BorderBrush='Black'  BorderThickness='0,0,1,1'/>
                    </Border>
                </Grid>
                <Label Grid.Row="9"  Grid.Column="1" Padding="0,0,0,0" Margin="0,2,3,0" HorizontalAlignment="Right" Content="Failure Rate [0.00001 - 100]:" FontSize="4" FontWeight="Bold" />
                <Grid Grid.Row="9"  Grid.Column="2" Background="{StaticResource brushWatermarkBackground}" >
                    <Border BorderBrush='WhiteSmoke'  BorderThickness='2,1,0,1'/>
                    <TextBlock Margin="5,2" Text="RS0" FontSize="4" Visibility="{Binding ElementName=RS0, Path=Text.IsEmpty, Converter={StaticResource BooleanToVisibilityConverter}}" Foreground="{StaticResource brushWatermarkForeground}"/>
                    <TextBox  x:Name="FR0" Text="{Binding FR0, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True,NotifyOnValidationError=True}"  Padding="0,0,0,0" Margin="2,1,0,1"  FontSize="4" Background="Transparent"   />
                </Grid>
                <Grid Grid.Row="9"  Grid.Column="3" Background="{StaticResource brushWatermarkBackground}" >
                    <Border BorderBrush='WhiteSmoke'  BorderThickness='2,1,0,1'/>
