                    <TextBox  x:Name="FR5" Text="{Binding FR5, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True,NotifyOnValidationError=True}"  Padding="0,0,0,0" Margin="2,1,0,1"  FontSize="4" Background="Transparent"  />
                </Grid>
